algoritmo "Desafio - Triângulo"
var

   contA, contB, num: inteiro

inicio

   escreva("Digite um número: ")
   leia(num)
   
   para contA <- 1 ate num faca
   
   
      para contB <- 1 ate contA faca

           escreva("*")
      
      fimpara
      
      escreval

   
   fimpara
   

fimalgoritmo