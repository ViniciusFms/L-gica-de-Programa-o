algoritmo "Operadores Realacionais"
var

   a, b: inteiro
   resultado: logico

inicio

   a <- 3
   b <- 7
   
   resultado <- 5 <= b - a

   // Relação entre valores estáticos
   escreval(5 > 2)
   
   // Relação entre valor estático e variável
   escreval(a < 3)
   
   // Relação entre variáveis
   escreval(b <> a)
   
   escreval(resultado)

fimalgoritmo