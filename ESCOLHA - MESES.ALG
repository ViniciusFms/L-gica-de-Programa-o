algoritmo "Escolha - meses"
var

   mes: inteiro

inicio

   escreva("Digite o número de um mês: ")
   leia(mes)

   escolha(mes)

   caso 11
      escreval("Mês de Novembro")
   caso 1
      escreval("Mês de Janeiro")
   caso 7
      escreval("Mês de Julho")
   outrocaso
      escreva("Mês não encontrado.")

   fimescolha

fimalgoritmo