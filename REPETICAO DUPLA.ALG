algoritmo "Repeticao dupla"
var

   contA, contB, soma: inteiro

inicio


   para contA <- 1 ate 3 faca
   
      //escreval("Contador A: ", contA)
   

      para contB <- 1 até 3 faca

         soma <- contA + contB
         
         escreval(contA," + ",contB," = ",soma)

      fimpara
      
      escreval
      

   fimpara


fimalgoritmo