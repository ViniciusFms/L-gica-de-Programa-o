algoritmo "Saída de dados com variaveis"
var

   // Declaração de variáveis
   nome: caracter

inicio

   // Atribuição de valor para variável
   nome <- "Harry"


   escreval("O valor da variável é: ", nome)
   
   nome <- "Bob"
   
   escreval("O valor da variável é: ", nome)

fimalgoritmo